Var
altura,base,area:real

Inicio

escreva("digite a altura")
leia(altura)
escreva("digite a base")
leia(base)

area <- (altura*base)/2;
escreval("resultado da área=",area)

escreval("Altura=",altura)
escreval("Base=",base)

// Fim

Fimalgoritmo
